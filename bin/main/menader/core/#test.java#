package test;

@SafePass
public class NamePass {

	@SelectXML(XPath.descendantOrSelf("names"))
	public void selectName(Element node, Marshaller m) {
		node.setText(m.getName());
	}

	public void apply(Document doc, Marshaller m) {
		var nodes = doc.selectNodes(tag);
		for (var node : nodes) {
			selectNames(node);
		}
	}
	
}